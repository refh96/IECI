{
    "Arrendatario": {
        "nombre": {
            "type": "String",
            "required": true
        },
        "apellido": {
            "type": "String",
            "required": true
        },
        "numero": {
            "type": "Number",
            "required": true
        },
        "correo": {
            "type":"String",
            "required": true
        },
        "status": {
            "type": "Schema.ObjectID",
            "ref": "Status"
        }
    },
    "status": {
        "name": {
            "type": "String",
            "required": true,
            "minLength": 1,
            "maxLength": 100,
            "enum": [
                "active",
                "inactive"
            ]
        }
    },

    "espacios":{
        "nombre":{
            "type":"String",
            "require":true
        },
        "cantidad":{
            "type":"Number",
            "require":true
        },
        "description":{
            "type":"String",
            "require":true
        }
    }
}
